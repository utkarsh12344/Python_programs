def myFunction():
    print("hello world")
myFunction()

def myFunction(fname):
    print(fname +"  " "labuschagne")
myFunction("marcus")

def myFunction(fname,lname):
    print(fname +"  " + lname)
myFunction("stveve","smith")

def my_function(*kids):
  print("The youngest child is " + kids[0])

my_function("Emil", "Tobias", "Linus")